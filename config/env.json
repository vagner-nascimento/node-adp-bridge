{
    "data": {
        "amqp": {
            "connStr": "amqp://guest:guest@localhost:5672",
            "connRetry": {
                "sleep": 4000,
                "maxTries": 10
            },
            "exitOnLostConnection": true
        }
    },
    "presentation": {
        "rest": {
            "port": 3000
        }
    },
    "integration": {
        "rest": {
            "merchants": {
                "baseUrl": "http://localhost:4000/merchants",
                "timeout": 10000
            },
            "merchantsAccounts": {
                "baseUrl": "http://localhost:4000/merchant-accounts",
                "timeout": 10000
            },
            "affiliations": {
                "baseUrl": "http://localhost:4000/affiliations",
                "timeout": 10000
            }
        },
        "amqp": {
            "rabbitMq": {
                "retryPolicy": "exit",
                "topics": {
                    "sub": {
                        "merchant": {
                            "connectionString": "amqp://guest:guest@localhost:5672",
                            "topicName": "q-merchants",
                            "subName": "c-merchant"
                        },
                        "seller": {
                            "connectionString": "amqp://guest:guest@localhost:5672",
                            "topicName": "q-sellers",
                            "subName": "c-seller"
                        }
                    },
                    "pub": {
                        "account": {
                            "connectionString": "amqp://guest:guest@localhost:5672",
                            "topicName": "q-accounts"
                        }
                    }
                }
            }
        }
    }
}

{
    "data": {
        "amqp": {
            "connStr": "amqp://guest:guest@localhost:5672"
        }
    },
    "presentation": {
        "rest": {
            "port": 3000
        }
    },
    "integration": {
        "rest": {
            "merchants": {
                "baseUrl": "http://localhost:4000/merchants",
                "timeout": 10000
            },
            "merchantsAccounts": {
                "baseUrl": "http://localhost:4000/merchant-accounts",
                "timeout": 10000
            },
            "affiliations": {
                "baseUrl": "http://localhost:4000/affiliations",
                "timeout": 10000
            }
        },
        "amqp": {
            "sub": {
                "merchant": {
                    "topic": "q-merchants",
                    "consumer": "c-merchant"
                },
                "seller": {
                    "topic": "q-sellers",
                    "consumer": "c-seller"
                }
            },
            "pub": {
                "account": {
                    "topic": "q-accounts"
                }
            }
        }
    }
}